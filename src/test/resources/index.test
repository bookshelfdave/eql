connect elasticsearch localhost:9300;

index posts with post = '{"xyz":"this is a test", "foobar":100}';
index posts with post = '{"xyz":"this is another test", "foobar":200}';

get posts with post = "AU3Po0OOZX4PYDrqsDN1";

query posts(xyz, foobar);

// xyz will be analyzed, so it might not return a value
// match on 100 for this example instead
query posts(xyz, foobar) filter foobar=100;